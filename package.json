{
  "name": "react-express-starter",
  "private": true,
  "version": "0.1.2",
  "description": "React Express Starter Kit",
  "main": "bin/www",
  "scripts": {
    "clean": "node_modules/.bin/rimraf ./__*__",
    "dev": "npm-run-all --parallel test:watch start:dev",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "bin/www",
    "start:development": "NODE_ENV=development DEBUG=server node_modules/nodemon/bin/nodemon.js bin/www",
    "start:production": "NODE_ENV=production bin/www",
    "test": "jest --config=jest.config.json",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "cp:nonjs": "rsync -a --prune-empty-dirs --include '*/' --include '*.gif' --include '*.ico' --include '*.jpg' --include '*.md' --include '*.png' --include '*.scss' --include '*.txt' --exclude '*' src/ __runtime__/",
    "cp:cleanscss": "find ./__runtime__/ -name *.scss -exec sed -e 's/.*//' {} + 1> /dev/null;",
    "cp:build": "cp ./__build__/* ./__runtime__/public",
    "preruntime": "npm run clean",
    "runtime": "npm run cp:nonjs && npm run cp:cleanscss; npm run runtime:client; npm run runtime:server; npm run cp:build;",
    "runtime:client": "webpack --config ./webpack.config.js",
    "runtime:server": "npx babel src/server/index.js src/ --out-dir __runtime__ --source-maps && cp -r ./config ./__runtime__ && cp -r ./src/server/views ./__runtime__/server/views",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o __storybook__"
  },
  "author": "Allen Evans",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "atob": "2.1.1",
    "body-parser": "1.18.3",
    "btoa": "1.2.1",
    "classnames": "2.2.5",
    "cookie-parser": "1.4.3",
    "debug": "3.1.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "history": "4.7.2",
    "node-fetch": "2.1.2",
    "nodemon": "1.17.5",
    "npm-run-all": "4.1.3",
    "pre-commit": "1.2.2",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-thunk": "2.3.0",
    "reselect": "3.0.1",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.0-alpha.8",
    "@storybook/addon-links": "4.0.0-alpha.8",
    "@storybook/addon-viewport": "4.0.0-alpha.8",
    "@storybook/addons": "4.0.0-alpha.8",
    "@storybook/react": "4.0.0-alpha.8",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-inline-import-data-uri": "1.0.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-react": "7.8.2",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.1.0",
    "node-sass": "4.9.0",
    "postcss-loader": "2.1.5",
    "react-test-renderer": "16.4.0",
    "resolve-url-loader": "2.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.2",
    "style-loader": "0.21.0",
    "supertest": "3.1.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.10.2",
    "webpack-cli": "3.0.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2",
    "webpack-manifest-plugin": "2.0.3",
    "webpack-merge": "4.1.2"
  },
  "pre-commit": {
    "silent": true,
    "run": [
      "lint",
      "test"
    ]
  }
}
